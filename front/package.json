{
  "name": "tkdo-front",
  "description": "Tirage au sort de cadeaux, en famille ou entre amis",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "npm run format && ng lint && ng test",
    "lint": "ng lint",
    "e2e": "npm run build && cypress run --config baseUrl=http://front",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "ct": "ng run tkdo-front:ct",
    "format": "npx prettier . --write | grep -v unchanged || true",
    "postinstall": "node version.js"
  },
  "private": true,
  "overrides": {
    "qs": ">=6.14.1"
  },
  "dependencies": {
    "@angular/animations": "~21.1.3",
    "@angular/cdk": "~21.0.6",
    "@angular/common": "~21.1.3",
    "@angular/compiler": "~21.1.3",
    "@angular/core": "~21.1.3",
    "@angular/forms": "~21.1.3",
    "@angular/platform-browser": "~21.1.3",
    "@angular/platform-browser-dynamic": "~21.1.3",
    "@angular/router": "~21.1.3",
    "@ng-bootstrap/ng-bootstrap": "^20.0.0",
    "@popperjs/core": "^2.11.8",
    "@types/jquery": "^3.5.29",
    "bootstrap": "^5.3.2",
    "git-describe": "^4.1.1",
    "jquery": "^3.7.1",
    "moment": "^2.30.1",
    "popper.js": "^1.16.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.1.3",
    "@angular-eslint/builder": "^21.1.0",
